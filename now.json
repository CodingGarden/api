{
  "version": 2,
  "name": "patreon",
  "builds": [
    {
      "src": "src/index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "src/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PATREON_ACCESS_TOKEN": "@patreon-access-token",
    "PATREON_CAMPAIGN_ID": "@patreon-campaign-id"
  },
  "alias": [
    "patreon.coding.garden"
  ]
}